{
  "name": "com.skyfiresolution.tokunabi.admin",
  "description": "Skyfire Solution Management System",
  "version": "1.0.0",
  "author": "Skyfire Solution Team",
  "homepage": "https://skyfiresolution.com",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "npm:check:update": "npm outdated -g --depth=0 && npm outdated",
    "env:dev": "cp -f src/environments/environment.develop.ts src/environments/environment.ts",
    "env:stag": "cp -f src/environments/environment.stag.ts src/environments/environment.ts",
    "app:start:dev": "npm run env:dev && npm run start",
    "app:start:stag": "npm run env:stag && npm run start",
    "app:deploy:stag": "rm -d -rf dist && ng build --configuration staging --build-optimizer",
    "app:deploy:prod": "rm -d -rf dist && ng build --configuration production --build-optimizer",
    "win:app:start:dev": "npm run start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.7",
    "@angular/cdk": "^12.2.7",
    "@angular/common": "~12.2.7",
    "@angular/compiler": "~12.2.7",
    "@angular/core": "~12.2.7",
    "@angular/forms": "~12.2.7",
    "@angular/localize": "~12.2.7",
    "@angular/material": "^12.2.7",
    "@angular/platform-browser": "~12.2.7",
    "@angular/platform-browser-dynamic": "~12.2.7",
    "@angular/router": "~12.2.7",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@tinymce/tinymce-angular": "^4.2.4",
    "angular-datatables": "^12.0.0",
    "angular-material-data-grid": "^0.3.2",
    "bootstrap": "^5.1.1",
    "datatables.net": "^1.10.20",
    "datatables.net-dt": "^1.10.20",
    "jquery": "^3.6.0",
    "ng-recaptcha": "^8.0.1",
    "ng2-completer": "^9.0.1",
    "ng2-smart-table": "^1.7.2",
    "ngx-toastr": "^14.1.3",
    "primeicons": "^4.1.0",
    "primeng": "^12.2.3",
    "rxjs": "~6.6.7",
    "sha.js": "^2.4.11",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular/cli": "~12.2.7",
    "@angular/compiler-cli": "~12.2.7",
    "@schematics/angular": "^12.2.16",
    "@types/datatables.net": "^1.10.18",
    "@types/jasmine": "~3.9.0",
    "@types/jquery": "^3.5.6",
    "@types/node": "^16.10.0",
    "@types/sha.js": "^2.4.0",
    "jasmine-core": "~3.9.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "typescript": "~4.3.5"
  },
  "main": "index.js",
  "repository": "https://github.com/enjoyvinh/torenta_admin.git",
  "license": "MIT"
}
